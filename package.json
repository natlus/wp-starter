{
  "name": "wp-starter",
  "version": "0.1.0",
  "description": "Fast WordPress starter theme using modern workflow",
  "main": "index.html",
  "scripts": {
    "start": "./node_modules/gulp/bin/gulp.js",
    "clean": "./node_modules/rimraf/bin.js build/",
    "build:css": "./node_modules/gulp/bin/gulp.js styles",
    "build:js": "./node_modules/gulp/bin/gulp.js scripts",
    "build:templates": "./node_modules/gulp/bin/gulp.js templates",
    "build:php": "./node_modules/gulp/bin/gulp.js includes",
    "build:images": "./node_modules/gulp/bin/gulp.js images",
    "build:fonts": "./node_modules/gulp/bin/gulp.js fonts",
    "build:lib": "./node_modules/gulp/bin/gulp.js vendor",
    "build:clean": "npm run clean && ./node_modules/gulp/bin/gulp.js build"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "browser-sync": "^2.17.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-load-plugins": "^1.3.0",
    "gulp-newer": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.1.1",
    "gulp-uglify": "^2.0.0",
    "rimraf": "^2.5.4",
    "sanitize.css": "^4.1.0"
  },
  "devDependencies": {},
  "author": "Jesper Sultan",
  "license": "ISC"
}
